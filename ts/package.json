{
  "name": "base62-ts",
  "version": "1.1.6",
  "description": "Custom Base-62 Encoder in TypeScript",
  "main": "dist/lib/src/typescript/index.js",
  "types": "dist/lib/src/typescript/index.d.ts",
  "scripts": {
    "browserify": "browserify lib/src/typescript/index.ts --debug -p tsify -s base62 -o dist/base62-${npm_package_version}.js",
    "minify": "terser dist/base62-${npm_package_version}.js -c -m -o dist/base62-${npm_package_version}.min.js",
    "compile": "eslint lib --ext .ts && tsc",
    "fix": "eslint lib --ext .ts --fix",
    "test": "tsc && mocha 'test/src/typescript/node.spec.ts' --require ts-node/register && browserify ./dist/test/src/typescript/browser.spec.js -o dist/test/src/typescript/index.js && live-server --port=9001 --mount=/:test/src/typescript",
    "test-browser": "browserify ./dist/test/src/typescript/browser.spec.js -o dist/test/src/typescript/index.js && live-server --port=9001 --mount=/:test/src/typescript",
    "test-node": "tsc && mocha 'test/src/typescript/node.spec.ts' --require ts-node/register",
    "fix-test": "eslint test --ext .ts --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cyrildever/base62.git"
  },
  "keywords": [
    "base62",
    "base-62",
    "encoder",
    "decoder"
  ],
  "author": "Cyril Dever <cdever@edgewhere.fr>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cyrildever/base62/issues"
  },
  "homepage": "https://github.com/cyrildever/base62#readme",
  "dependencies": {
    "ts-utls": "^1.1.2"
  },
  "devDependencies": {
    "@types/chai": "^4.2.15",
    "@types/mocha": "^8.2.1",
    "@types/node": "^14.14.34",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "browserify": "^17.0.0",
    "chai": "^4.3.4",
    "eslint": "^7.22.0",
    "eslint-plugin-no-loops": "^0.3.0",
    "mocha": "^8.3.2",
    "terser": "^5.6.0",
    "ts-node": "^9.1.1",
    "tsify": "^5.0.2",
    "typescript": "~3.8.3"
  }
}